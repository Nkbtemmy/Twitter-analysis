{
	"name": "twitter-analysis-app",
	"version": "1.0.0",
	"description": "twitter-analysis-app",
	"main": "src/server.ts",
	"repository": {
		"type": "git",
		"url": "git@github.com:AmaliTech-Training-Rw/storefront-rw-backend.git"
	},
	"author": "",
	"license": "ISC",
	"lint-staged": {
		"*.{js,css,md}": "prettier --ignore-unknown --write"
	},
	"scripts": {
		"dev": "nodemon",
		"ts:dev": "ts-node-dev --respawn --rs --watch src src/server.ts",
		"start": "node build/server.js",
		"build": "tsc",
		"clean": "rimraf build",
		"prebuild": "yarn clean",
		"db:migrate": "sequelize db:migrate",
		"db:seed": "sequelize db:seed:all",
		"db:unseed": "sequelize db:seed:undo:all",
		"db:migrate:undo": "sequelize db:migrate:undo:all",
		"db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
		"db:hard:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
		"db:create": "sequelize db:create",
		"db:drop": "sequelize db:drop",
		"format": "prettier --write .",
		"prepare": "husky install",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint --fix --ext .ts",
		"test": "PORT=3021 jest --runInBand --",
		"test:watch": "jest --runInBand --watchAll",
		"db:make_migration": " sequelize migration:generate --name"
	},
	"dependencies": {
		"@types/body-parser": "^1.19.3",
		"@types/express": "^4.17.18",
		"@types/express-session": "^1.17.8",
		"@types/node": "^20.8.0",
		"@types/pg": "^8.10.3",
		"@types/sequelize": "^4.28.16",
		"@types/socket.io": "^3.0.2",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.33.0",
		"socket.io": "^4.7.2",
		"swagger-ui-express": "^5.0.0",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.23.0",
		"@types/bcryptjs": "^2.4.4",
		"@types/cors": "^2.8.14",
		"@types/express-fileupload": "^1.4.2",
		"@types/jest": "^29.5.5",
		"@types/jsonwebtoken": "^9.0.3",
		"@types/morgan": "^1.9.6",
		"@types/supertest": "^2.0.16",
		"@types/swagger-ui-express": "^4.1.4",
		"@types/uuid": "^9.0.6",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.3",
		"eslint": "^8.50.0",
		"eslint-plugin-jest": "^27.4.2",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^14.0.1",
		"nodemon": "^3.0.1",
		"prettier": "3.0.3",
		"sequelize-cli": "^6.6.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tslint": "^6.1.3",
		"typescript": "^5.2.2"
	}
}
